<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/green_light25</item>
        <item name="colorPrimaryVariant">@color/bleu</item>
        <item name="colorOnPrimary">@color/back1</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/green_light25</item>
        <item name="colorSecondaryVariant">@color/green_light24</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowContentTransitions">true</item>
        <item name="android:colorBackground">@color/white</item>

    </style>
</resources>

